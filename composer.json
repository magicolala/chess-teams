{
    "name": "magicolala/chess-teams",
    "description": "Application web d’échecs collaboratifs (Symfony 7, PHP 8.4+).",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "chesslablab/php-chess": "^1.6",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.15",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.5",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.2",
        "predis/predis": "^3.2",
        "ramsey/uuid-doctrine": "^2.1",
        "symfony/asset": "^7.0",
        "symfony/asset-mapper": "^7.0",
        "symfony/console": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/lock": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7.0",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/stimulus-bundle": "^2.29",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/ux-turbo": "^2.29",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/yaml": "^7.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "secure-http": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs:fix": [
            "php tools/php-cs-fixer.phar fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
        ],
        "cs:check": [
            "php tools/php-cs-fixer.phar fix --config=.php-cs-fixer.dist.php --allow-risky=yes --dry-run --diff"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse src tests --memory-limit=512M"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.86",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.13",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "lcobucci/jwt",
                "version": "5.3.0",
                "type": "library",
                "source": {
                    "type": "git",
                    "url": "https://github.com/lcobucci/jwt.git",
                    "reference": "08071d8d2c7f4b00222cc4b1fb6aa46990a80f83"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://api.github.com/repos/lcobucci/jwt/zipball/08071d8d2c7f4b00222cc4b1fb6aa46990a80f83",
                    "reference": "08071d8d2c7f4b00222cc4b1fb6aa46990a80f83",
                    "shasum": ""
                },
                "require": {
                    "ext-openssl": "*",
                    "ext-sodium": "*",
                    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
                    "psr/clock": "^1.0"
                },
                "autoload": {
                    "psr-4": {
                        "Lcobucci\\JWT\\": "src"
                    }
                },
                "license": [
                    "BSD-3-Clause"
                ],
                "description": "A simple library to work with JSON Web Token and JSON Web Signature"
            }
        },
        {
            "type": "composer",
            "url": "https://repo.packagist.org"
        }
    ]
}
